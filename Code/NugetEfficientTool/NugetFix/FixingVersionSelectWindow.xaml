<Window x:Class="NugetEfficientTool.FixingVersionSelectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="请选择 Nuget 版本" Height="300" Width="600" WindowStartupLocation="CenterOwner" ShowInTaskbar="False">
    <Grid>
        <ScrollViewer HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Auto">
            <WrapPanel Name="NugetVersionsPanel" FlowDirection="LeftToRight" HorizontalAlignment="Left"/>
        </ScrollViewer>
        <Button Name="ButtonFix" Margin="10" Height="26" Content="执行修复" HorizontalAlignment="Center"
                VerticalAlignment="Bottom" Click="ButtonFix_OnClick"
                Style="{StaticResource Style.Button.AccentBackground.Text.WhiteTextAndAccentBackground}"/>
    </Grid>
</Window>